{
  "name": "mobile-links-website",
  "version": "0.1.0",
  "private": true,
  "description": "Generate a static page with links to your websites for e.g social media accounts.",
  "homepage": "https://github.com/danielheene/mobile-links-website",
  "bugs": "https://github.com/danielheene/mobile-links-website/issues",
  "repository": "github:danielheene/mobile-links-website",
  "license": "MIT",
  "author": "Daniel Heene <mail@danielheene.de>",
  "scripts": {
    "prebuild": "run-p clean:*",
    "build": "gatsby build",
    "postbuild": "echo \"dnlsn.one\" > public/CNAME",
    "clean": "run-p clean:*",
    "clean:gatsby": "gatsby clean",
    "clean:logs": "rimraf *.log || exit 0",
    "format": "run-s format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:package": "sort-package-json",
    "format:stylelint": "yarn lint:stylelint --fix",
    "postinstall": "run-s types:install format:package",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --ext=json --ext=js --ext=jsx --ext=ts --ext=tsx .",
    "lint:stylelint": "stylelint \"src/**/*.{css,sass,less,js,jsx,ts,tsx}\"",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0",
    "types:install": "typesync && yarn install --ignore-scripts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "bracketSpacing": true,
    "endOfLine": "lf",
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "stylelint": {
    "plugins": [
      "stylelint-order"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-recess-order"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.35",
    "deepmerge": "4.2.2",
    "gatsby": "2.24.79",
    "gatsby-plugin-emotion": "4.3.14",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-typescript": "2.4.24",
    "keypress": "^0.2.1",
    "lodash.merge": "4.6.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet": "6.1.0",
    "react-icons": "3.11.0",
    "rxjs": "6.6.3"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/babel__core": "7.1.10",
    "@types/clear": "^0.1.0",
    "@types/eslint": "7.2.4",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/github-url-to-object": "^4.0.0",
    "@types/inquirer": "7.3.1",
    "@types/listr": "0.14.2",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^14.14.5",
    "@types/node-fetch": "^2.5.7",
    "@types/prettier": "2.1.2",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@types/remarkable": "^1.7.4",
    "@types/rimraf": "3.0.0",
    "@types/showdown": "^1.9.3",
    "@types/stylelint": "9.10.1",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "boxen": "^4.2.0",
    "clean-deep": "^3.4.0",
    "clear": "^0.1.0",
    "eslint": "7.12.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "get-port": "^5.1.1",
    "github-markdown-css": "^4.0.0",
    "github-url-to-object": "^4.0.4",
    "husky": "4.3.0",
    "inquirer": "^7.3.3",
    "inquirer-loop": "^0.3.0",
    "inquirer-maxlength-input-prompt": "^1.0.1",
    "inquirer-recursive": "^0.0.6",
    "inquirer-search-list": "^1.2.6",
    "kleur": "^4.1.3",
    "listr": "0.14.3",
    "markdown-toc": "1.2.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "4.1.5",
    "open": "7.3.0",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "sort-package-json": "1.46.1",
    "stylelint": "13.7.2",
    "stylelint-config-recess-order": "2.3.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "terminal-link": "^2.1.1",
    "ts-node": "^9.0.0",
    "typescript": "4.0.3",
    "typesync": "0.7.0"
  }
}
