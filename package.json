{
  "name": "dnlsn.one",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "license": "0BSD",
  "author": "Daniel Heene <mail@danielheene.de>",
  "scripts": {
    "prebuild": "run-p clean:*",
    "build": "gatsby build",
    "postbuild": "echo \"dnlsn.one\" > public/CNAME",
    "clean": "run-p clean:*",
    "clean:gatsby": "gatsby clean",
    "clean:logs": "rimraf *.log || exit 0",
    "format": "run-s format:*",
    "format:package": "sort-package-json",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "postinstall": "run-s types:install format:*",
    "lint": "run-s lint:*",
    "lint:eslint": "echo \"Add eslint!\" && exit 1",
    "lint:stylelint": "echo \"Add stylelint!\" && exit 1",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "types:install": "typesync && yarn install --ignore-scripts"
  },
  "prettier": {
    "bracketSpacing": true,
    "endOfLine": "lf",
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@emotion/core": "10.0.35",
    "@emotion/styled": "10.0.27",
    "gatsby": "2.24.79",
    "gatsby-image": "2.4.21",
    "gatsby-plugin-emotion": "4.3.14",
    "gatsby-plugin-manifest": "2.4.34",
    "gatsby-plugin-offline": "3.2.33",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-typescript": "2.4.24",
    "gatsby-source-filesystem": "2.3.34",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@types/babel__core": "^7.1.10",
    "@types/node": "14.14.2",
    "@types/prettier": "2.1.2",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@types/rimraf": "^3.0.0",
    "npm-run-all": "latest",
    "prettier": "2.1.2",
    "rimraf": "^3.0.2",
    "sort-package-json": "latest",
    "typescript": "4.0.3",
    "typesync": "latest"
  }
}
